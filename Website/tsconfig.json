{
    "compileOnSave": true,
    "compilerOptions": {
        "outDir": "wwwroot/ts",
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "target": "es5",
        "typeRoots": [ "./typings" ]
    },
    "include": [ "Typescript/**/*" ],
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}
